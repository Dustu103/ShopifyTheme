{% doc %}
  @prompt
    An interactive vertical timeline that highlights as the user scrolls, remove this ...this should be a slidide of carousel with some logo images
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-logo-carousel-{{ ai_gen_id }} {
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
    padding: {{ block.settings.section_padding }}px 20px;
    background-color: {{ block.settings.background_color }};
    overflow: hidden;
  }

  .ai-logo-carousel-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: {{ block.settings.header_margin_bottom }}px;
  }

  .ai-logo-carousel-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_font_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 {{ block.settings.title_margin_bottom }}px;
    font-weight: {{ block.settings.title_font_weight }};
  }

  .ai-logo-carousel-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_font_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
  }

  .ai-logo-carousel-container-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.carousel_border_radius }}px;
  }

  .ai-logo-carousel-track-{{ ai_gen_id }} {
    display: flex;
    transition: transform {{ block.settings.transition_duration | divided_by: 1000.0 }}s ease-in-out;
    gap: {{ block.settings.logo_spacing }}px;
  }

  .ai-logo-carousel-slide-{{ ai_gen_id }} {
    flex: 0 0 calc((100% - {{ block.settings.logo_spacing | times: block.settings.logos_per_view | minus: block.settings.logo_spacing }}px) / {{ block.settings.logos_per_view }});
    display: flex;
    align-items: center;
    justify-content: center;
    padding: {{ block.settings.logo_padding }}px;
    background-color: {{ block.settings.slide_background }};
    border-radius: {{ block.settings.slide_border_radius }}px;
    border: {{ block.settings.slide_border_width }}px solid {{ block.settings.slide_border_color }};
    transition: all 0.3s ease;
  }

  .ai-logo-carousel-slide-{{ ai_gen_id }}:hover {
    transform: translateY(-{{ block.settings.hover_lift }}px);
    box-shadow: 0 {{ block.settings.hover_shadow_y }}px {{ block.settings.hover_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.hover_shadow_opacity | divided_by: 100.0 }});
  }

  .ai-logo-carousel-logo-{{ ai_gen_id }} {
    max-width: 100%;
    max-height: {{ block.settings.logo_height }}px;
    width: auto;
    height: auto;
    object-fit: contain;
    filter: grayscale({{ block.settings.logo_grayscale }}%) opacity({{ block.settings.logo_opacity }}%);
    transition: filter 0.3s ease;
  }

  .ai-logo-carousel-slide-{{ ai_gen_id }}:hover .ai-logo-carousel-logo-{{ ai_gen_id }} {
    filter: grayscale({{ block.settings.logo_hover_grayscale }}%) opacity({{ block.settings.logo_hover_opacity }}%);
  }

  .ai-logo-carousel-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: {{ block.settings.logo_height }}px;
    background-color: #f4f4f4;
    border-radius: 4px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #999;
    font-size: 12px;
  }

  .ai-logo-carousel-controls-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 20px;
    margin-top: {{ block.settings.controls_margin_top }}px;
  }

  .ai-logo-carousel-nav-{{ ai_gen_id }} {
    background: {{ block.settings.nav_button_color }};
    border: none;
    border-radius: 50%;
    width: {{ block.settings.nav_button_size }}px;
    height: {{ block.settings.nav_button_size }}px;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all 0.3s ease;
    color: {{ block.settings.nav_button_icon_color }};
  }

  .ai-logo-carousel-nav-{{ ai_gen_id }}:hover {
    background: {{ block.settings.nav_button_hover_color }};
    transform: scale(1.1);
  }

  .ai-logo-carousel-nav-{{ ai_gen_id }}:disabled {
    opacity: 0.5;
    cursor: not-allowed;
    transform: none;
  }

  .ai-logo-carousel-dots-{{ ai_gen_id }} {
    display: flex;
    gap: 8px;
  }

  .ai-logo-carousel-dot-{{ ai_gen_id }} {
    width: {{ block.settings.dot_size }}px;
    height: {{ block.settings.dot_size }}px;
    border-radius: 50%;
    background: {{ block.settings.dot_color }};
    border: none;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-logo-carousel-dot-{{ ai_gen_id }}.active {
    background: {{ block.settings.dot_active_color }};
    transform: scale(1.2);
  }

  .ai-logo-carousel-autoplay-toggle-{{ ai_gen_id }} {
    background: {{ block.settings.autoplay_button_color }};
    color: {{ block.settings.autoplay_button_text_color }};
    border: none;
    padding: 8px 16px;
    border-radius: {{ block.settings.autoplay_button_radius }}px;
    cursor: pointer;
    font-size: 12px;
    transition: all 0.3s ease;
  }

  .ai-logo-carousel-autoplay-toggle-{{ ai_gen_id }}:hover {
    background: {{ block.settings.autoplay_button_hover_color }};
  }

  @media screen and (max-width: 749px) {
    .ai-logo-carousel-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding | times: 0.7 }}px 15px;
    }

    .ai-logo-carousel-track-{{ ai_gen_id }} {
      gap: {{ block.settings.logo_spacing | times: 0.7 }}px;
    }

    .ai-logo-carousel-slide-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - {{ block.settings.logo_spacing | times: 0.7 | times: block.settings.mobile_logos_per_view | minus: block.settings.logo_spacing | times: 0.7 }}px) / {{ block.settings.mobile_logos_per_view }});
      padding: {{ block.settings.logo_padding | times: 0.8 }}px;
    }

    .ai-logo-carousel-logo-{{ ai_gen_id }} {
      max-height: {{ block.settings.logo_height | times: 0.8 }}px;
    }

    .ai-logo-carousel-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_font_size | times: 0.9 }}px;
    }

    .ai-logo-carousel-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_font_size | times: 0.9 }}px;
    }
  }
{% endstyle %}

<logo-carousel-{{ ai_gen_id }}
  class="ai-logo-carousel-{{ ai_gen_id }}"
  data-autoplay="{{ block.settings.autoplay }}"
  data-autoplay-speed="{{ block.settings.autoplay_speed }}"
  data-logos-per-view="{{ block.settings.logos_per_view }}"
  data-mobile-logos-per-view="{{ block.settings.mobile_logos_per_view }}"
  {{ block.shopify_attributes }}
>
  {% if block.settings.show_header %}
    <div class="ai-logo-carousel-header-{{ ai_gen_id }}">
      {% if block.settings.title != blank %}
        <h2 class="ai-logo-carousel-title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
      {% endif %}
      {% if block.settings.subtitle != blank %}
        <p class="ai-logo-carousel-subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
      {% endif %}
    </div>
  {% endif %}

  <div class="ai-logo-carousel-container-{{ ai_gen_id }}">
    <div class="ai-logo-carousel-track-{{ ai_gen_id }}">
      {% for i in (1..12) %}
        {% liquid
          assign logo_key = 'logo_' | append: i
          assign url_key = 'logo_' | append: i | append: '_url'
          assign alt_key = 'logo_' | append: i | append: '_alt'
          assign enabled_key = 'logo_' | append: i | append: '_enabled'

          assign logo_image = block.settings[logo_key]
          assign logo_url = block.settings[url_key]
          assign logo_alt = block.settings[alt_key]
          assign logo_enabled = block.settings[enabled_key]
        %}

        {% if logo_enabled %}
          <div class="ai-logo-carousel-slide-{{ ai_gen_id }}">
            {% if logo_url != blank %}
              <a href="{{ logo_url }}" target="_blank" rel="noopener">
            {% endif %}
            
            {% if logo_image %}
              <img
                src="{{ logo_image | image_url: width: 300 }}"
                alt="{{ logo_alt | default: 'Partner logo' | escape }}"
                class="ai-logo-carousel-logo-{{ ai_gen_id }}"
                loading="lazy"
                width="300"
                height="{{ block.settings.logo_height }}"
              >
            {% else %}
              <div class="ai-logo-carousel-placeholder-{{ ai_gen_id }}">
                Logo {{ i }}
              </div>
            {% endif %}
            
            {% if logo_url != blank %}
              </a>
            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </div>

  {% if block.settings.show_controls %}
    <div class="ai-logo-carousel-controls-{{ ai_gen_id }}">
      {% if block.settings.show_nav_buttons %}
        <button class="ai-logo-carousel-nav-{{ ai_gen_id }} ai-logo-carousel-prev-{{ ai_gen_id }}" aria-label="Previous">
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <polyline points="15,18 9,12 15,6"></polyline>
          </svg>
        </button>
      {% endif %}

      {% if block.settings.show_dots %}
        <div class="ai-logo-carousel-dots-{{ ai_gen_id }}"></div>
      {% endif %}

      {% if block.settings.show_nav_buttons %}
        <button class="ai-logo-carousel-nav-{{ ai_gen_id }} ai-logo-carousel-next-{{ ai_gen_id }}" aria-label="Next">
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <polyline points="9,18 15,12 9,6"></polyline>
          </svg>
        </button>
      {% endif %}

      {% if block.settings.show_autoplay_toggle %}
        <button class="ai-logo-carousel-autoplay-toggle-{{ ai_gen_id }}">
          <span class="play-text">Play</span>
          <span class="pause-text" style="display: none;">Pause</span>
        </button>
      {% endif %}
    </div>
  {% endif %}
</logo-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class LogoCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentSlide = 0;
        this.totalSlides = 0;
        this.logosPerView = 4;
        this.mobileLogosPerView = 2;
        this.autoplayInterval = null;
        this.isAutoplay = false;
        this.autoplaySpeed = 3000;
      }

      connectedCallback() {
        this.track = this.querySelector('.ai-logo-carousel-track-{{ ai_gen_id }}');
        this.slides = Array.from(this.querySelectorAll('.ai-logo-carousel-slide-{{ ai_gen_id }}'));
        this.prevBtn = this.querySelector('.ai-logo-carousel-prev-{{ ai_gen_id }}');
        this.nextBtn = this.querySelector('.ai-logo-carousel-next-{{ ai_gen_id }}');
        this.dotsContainer = this.querySelector('.ai-logo-carousel-dots-{{ ai_gen_id }}');
        this.autoplayToggle = this.querySelector('.ai-logo-carousel-autoplay-toggle-{{ ai_gen_id }}');

        this.logosPerView = parseInt(this.dataset.logosPerView) || 4;
        this.mobileLogosPerView = parseInt(this.dataset.mobileLogosPerView) || 2;
        this.isAutoplay = this.dataset.autoplay === 'true';
        this.autoplaySpeed = parseInt(this.dataset.autoplaySpeed) || 3000;

        this.init();
      }

      init() {
        if (this.slides.length === 0) return;

        this.updateLogosPerView();
        this.calculateTotalSlides();
        this.createDots();
        this.setupEventListeners();
        this.updateCarousel();

        if (this.isAutoplay) {
          this.startAutoplay();
        }

        window.addEventListener('resize', () => {
          this.updateLogosPerView();
          this.calculateTotalSlides();
          this.createDots();
          this.updateCarousel();
        });
      }

      updateLogosPerView() {
        this.currentLogosPerView = window.innerWidth <= 749 ? this.mobileLogosPerView : this.logosPerView;
      }

      calculateTotalSlides() {
        this.totalSlides = Math.max(0, this.slides.length - this.currentLogosPerView + 1);
      }

      createDots() {
        if (!this.dotsContainer) return;

        this.dotsContainer.innerHTML = '';
        
        for (let i = 0; i < this.totalSlides; i++) {
          const dot = document.createElement('button');
          dot.className = `ai-logo-carousel-dot-{{ ai_gen_id }} ${i === 0 ? 'active' : ''}`;
          dot.setAttribute('aria-label', `Go to slide ${i + 1}`);
          dot.addEventListener('click', () => this.goToSlide(i));
          this.dotsContainer.appendChild(dot);
        }
      }

      setupEventListeners() {
        if (this.prevBtn) {
          this.prevBtn.addEventListener('click', () => this.prevSlide());
        }

        if (this.nextBtn) {
          this.nextBtn.addEventListener('click', () => this.nextSlide());
        }

        if (this.autoplayToggle) {
          this.autoplayToggle.addEventListener('click', () => this.toggleAutoplay());
        }

        this.addEventListener('mouseenter', () => this.pauseAutoplay());
        this.addEventListener('mouseleave', () => {
          if (this.isAutoplay) this.startAutoplay();
        });
      }

      updateCarousel() {
        if (!this.track) return;

        const slideWidth = 100 / this.currentLogosPerView;
        const translateX = -this.currentSlide * slideWidth;
        this.track.style.transform = `translateX(${translateX}%)`;

        this.updateDots();
        this.updateNavButtons();
      }

      updateDots() {
        if (!this.dotsContainer) return;

        const dots = this.dotsContainer.querySelectorAll('.ai-logo-carousel-dot-{{ ai_gen_id }}');
        dots.forEach((dot, index) => {
          dot.classList.toggle('active', index === this.currentSlide);
        });
      }

      updateNavButtons() {
        if (this.prevBtn) {
          this.prevBtn.disabled = this.currentSlide === 0;
        }

        if (this.nextBtn) {
          this.nextBtn.disabled = this.currentSlide >= this.totalSlides - 1;
        }
      }

      goToSlide(index) {
        this.currentSlide = Math.max(0, Math.min(index, this.totalSlides - 1));
        this.updateCarousel();
      }

      nextSlide() {
        if (this.currentSlide < this.totalSlides - 1) {
          this.currentSlide++;
        } else {
          this.currentSlide = 0;
        }
        this.updateCarousel();
      }

      prevSlide() {
        if (this.currentSlide > 0) {
          this.currentSlide--;
        } else {
          this.currentSlide = this.totalSlides - 1;
        }
        this.updateCarousel();
      }

      startAutoplay() {
        this.pauseAutoplay();
        this.autoplayInterval = setInterval(() => {
          this.nextSlide();
        }, this.autoplaySpeed);

        if (this.autoplayToggle) {
          this.autoplayToggle.querySelector('.play-text').style.display = 'none';
          this.autoplayToggle.querySelector('.pause-text').style.display = 'inline';
        }
      }

      pauseAutoplay() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
          this.autoplayInterval = null;
        }

        if (this.autoplayToggle) {
          this.autoplayToggle.querySelector('.play-text').style.display = 'inline';
          this.autoplayToggle.querySelector('.pause-text').style.display = 'none';
        }
      }

      toggleAutoplay() {
        this.isAutoplay = !this.isAutoplay;
        
        if (this.isAutoplay) {
          this.startAutoplay();
        } else {
          this.pauseAutoplay();
        }
      }

      disconnectedCallback() {
        this.pauseAutoplay();
      }
    }

    customElements.define('logo-carousel-{{ ai_gen_id }}', LogoCarousel{{ ai_gen_id }});
  })();
</script>

